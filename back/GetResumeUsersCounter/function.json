{
  "scriptFile": "__init__.py",
  "bindings": [
    {
      "authLevel": "anonymous",
      "type": "httpTrigger",
      "direction": "in",
      "name": "req",
      "methods": [
        "get",
        "post"
      ]
    },
    {
      "type": "http",
      "direction": "out",
      "name": "$return"
    },
    {
      "type": "cosmosDB",
      "direction": "in",
      "name": "inputDocument",
      "databaseName": "flo-resume-db",
      "collectionName": "configuration",
      "connectionStringSetting": "CosmosDbConnectionString",
      "partitionKey": "1",
      "id": "1"
    }
  ]
}
